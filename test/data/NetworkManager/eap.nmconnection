[connection]
id=eap-sample
uuid=7ca355e7-7989-465e-962e-9cb4df988e1a
type=wifi
permissions=user:imobach:;

[wifi]
mac-address-blacklist=
mode=infrastructure
ssid=MYWIFI

[wifi-security]
key-mgmt=wpa-eap

[802-1x]
ca-cert=/etc/ssl/certs/myca.pem
client-cert=/etc/ssl/certs/mywifi.pem
domain-suffix-match=suse.com
eap=tls;
identity=my-identity
private-key=/etc/ssl/private/mywifi.key
private-key-password=12345678

[ipv4]
dns-search=
method=auto

[ipv6]
addr-gen-mode=stable-privacy
dns-search=
method=auto

[proxy]
